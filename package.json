{
  "author": "virtualpatterns.com <code@virtualpatterns.com> (http://www.virtualpatterns.com)",
  "ava": {
    "failFast": true,
    "files": [
      "release/test/**/*.test.*"
    ],
    "nodeArguments": [
      "--no-warnings",
      "--unhandled-rejections=strict"
    ],
    "require": [
      "@virtualpatterns/mablung-source-map-support/install"
    ],
    "verbose": true
  },
  "babel": {
    "presets": [
      "@virtualpatterns/babel-preset-mablung-makefile"
    ],
    "env": {
      "commonjs": {
        "plugins": [
          "@babel/plugin-proposal-export-default-from"
        ],
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": {
                "node": "current"
              }
            }
          ]
        ]
      },
      "esmodule": {}
    }
  },
  "c8": {
    "all": true,
    "exclude": [
      "release/sandbox"
    ],
    "reporter": [
      "html"
    ],
    "src": "release"
  },
  "description": "TBD",
  "eslintConfig": {
    "extends": "@virtualpatterns/eslint-config-mablung-makefile",
    "parserOptions": {
      "babelOptions": {
        "parserOpts": {
          "plugins": [
            "exportDefaultFrom"
          ]
        }
      }
    }
  },
  "exports": {
    ".": "./release/index.cjs",
    "./package.json": "./release/index.cjs"
  },
  "files": [
    "/release/library",
    "/release/index.cjs"
  ],
  "license": "GPL-3.0+",
  "main": "release/index.cjs",
  "name": "@virtualpatterns/mablung-babel-plugin-replace-string-literal",
  "repository": {
    "type": "git",
    "url": "https://github.com/virtualpatterns/mablung-babel-plugin-replace-string-literal"
  },
  "type": "module",
  "version": "0.0.1-26",
  "dependencies": {
    "@pwn/is": "^1.0.2",
    "@virtualpatterns/mablung-babel-plugin": "0.0.1-31"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/plugin-proposal-export-default-from": "^7.14.5",
    "@babel/preset-env": "^7.14.8",
    "@virtualpatterns/babel-preset-mablung-makefile": "0.0.1-1",
    "@virtualpatterns/eslint-config-mablung-makefile": "0.0.1-12",
    "@virtualpatterns/mablung-check-dependency": "0.0.1-63",
    "@virtualpatterns/mablung-makefile": "0.0.1-93",
    "@virtualpatterns/mablung-source-map-support": "0.0.2-67",
    "ava": "^3.15.0"
  }
}
